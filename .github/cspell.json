{
  "version": "0.2",
  "language": "en",
  "flagWords": [],
  "words": [
    "ASDW",
    "backtick",
    "blockquote",
    "broadphase",
    "conformant",
    "dashbook",
    "fireslime",
    "flippable",
    "hitbox",
    "linkcheck",
    "LTRBR",
    "LTWH",
    "markdownlint",
    "positionable",
    "prerendered",
    "pressable",
    "proxying",
    "raycast",
    "raytracing",
    "refactor",
    "renderable",
    "socio",
    "Strikethrough",
    "stylesheet",
    "typedef",
    "toctree",
    "vsync"
  ],
  "ignorePaths": ["**/media/**", "**/assets/**", "**/test/**"],
  "ignoreRegExpList": ["@\\w+", "\\$\\w+", "\\{\\w+", "\\`\\w+", "\\:\\w+", "\\/\\w+", "\\#\\w+"],
  "dictionaries": ["en_US", "softwareTerms"],
  "languageSettings": [
      {
          "languageId": "markdown",
          // Exclude code in tripple backticks.
          "ignoreRegExpList": [
              "/^\\s*```[\\s\\S]*?^\\s*```/gm"
          ]
      }
  ]
}
